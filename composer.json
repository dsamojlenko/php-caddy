{
  "name": "samojled/php-caddy",
  "description": "A tiny php development environment for Windows",
  "keywords": ["laravel", "windows", "caddy", "caddyserver"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dave Samojlenko",
      "email": "dsamojlenko@gmail.com"
    }
  ],
  "autoload": {
    "files": [
      "cli/includes/compatibility.php",
      "cli/includes/constants.php",
      "cli/drivers/require.php"
    ],
    "psr-4": {
      "Caddy\\": "cli/Caddy/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "mnapoli/silly": "~1.0",
    "symfony/process": "~2.7|~3.0",
    "mnapoli/silly-php-di": "^1.1",
    "tightenco/collect": "^5.2"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": ">=4.8.24"
  },
  "bin": [
    "caddy"
  ]
}
